    "typescript": "^3.9.5",
    "vue-eslint-parser": "^7.1.0",
    "vue-jest": "^3.0.5",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11",
    "vuex-module-decorators": "^0.17.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
