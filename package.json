{
  "name": "client",
  "version": "0.5.8",
  "description": "ERT Notes allows you to generate websites using snippets based on the Bulma CSS Frameworks.",
  "homepage": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks",
  "bugs": {
    "url": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks/issues"
  },
  "private": true,
  "snyk": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/BulmaBuildingBlocks/BulmaBuildingBlocks.git"
  },
  "author": "Joseph Anson <me@josephanson.com>",
  "maintainers": [
    "Joseph Anson <me@BulmaBuildingBlocks.com>"
  ],
  "keywords": [
    "bulma",
    "vue",
    "vuejs",
    "vue-bulma",
    "components",
    "html",
    "html-snippets",
    "page-builder"
  ],
  "scripts": {
    "build": "concurrently \"yarn run server:build:migrate\" \"yarn run client:build\"",
    "ci": "concurrently \"yarn run lint\" \"yarn run unit -- --runInBand\"",
    "clean": "rimraf ./coverage .nuxt dist",
    "client:build": "nuxt-ts build",
    "client:dev": "nuxt-ts",
    "client:start": "nuxt-ts start",
    "e2e": "percy exec -- cypress open --browser chrome",
    "e2e:headless": "cypress run",
    "e2e:headless:visual": "percy exec -- cypress run",
    "format": "concurrently \"yarn run format:scripts\" \"yarn run format:scss\"",
    "format:scripts": "prettier --check **/*.{vue,ts,scss,json} --ignore-path .gitignore --write",
    "format:scss": "npx stylelint **/*.scss --fix",
    "lint": "eslint --ext .vue,.ts --ignore-path .gitignore . --fix",
    "lint-watch": "esw --ext .vue,.ts --ignore-path .gitignore . --fix --watch",
    "migration:generate": "yarn run typeorm migration:generate -n Migration",
    "migration:new": "yarn run typeorm migration:create -n Migration",
    "migration:revert": "yarn run typeorm migration:revert",
    "migration:run": "yarn run typeorm migration:run",
    "postci": "codecov",
    "prebuild": "yarn run clean",
    "precommit": "yarn run test",
    "predev": "yarn run clean",
    "release:major": "npm run build:scss && changelog -M && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version major",
    "release:minor": "npm run build:scss && changelog -m && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version minor",
    "release:patch": "npm run build:scss && changelog -p && git add CHANGELOG.md && git commit -m \"chore: update changelog\" && npm version patch",
    "server:build": "nest build",
    "server:build:migrate": "nest build && yarn migration:run",
    "server:debug": "nest start --debug --watch",
    "server:dev": "nest start --watch",
    "server:prod": "node dist/server/main",
    "start": "cross-env NODE_ENV=production yarn run server:prod",
    "start:dev": "cross-env NODE_ENV=development yarn prebuild && webpack --config webpack-hmr.config.js",
    "test": "cross-env NODE_ENV=test concurrently \"yarn run lint\" \"yarn run unit\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "unit": "cross-env NODE_ENV=test jest",
    "unit:update": "cross-env NODE_ENV=test jest --u",
    "unit:watch": "cross-env NODE_ENV=test jest --watchAll --coverage=false",
    "snyk-protect": "snyk protect"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run precommit"
    }
  },
  "resolutions": {
    "prosemirror-model": "1.9.1",
    "prosemirror-state": "1.3.3",
    "prosemirror-transform": "1.2.4",
    "prosemirror-view": "1.14.6"
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "^1.3.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-fastify": "^7.1.3",
    "@nestjs/typeorm": "^7.1.0",
    "@nuxt/builder": "^2.12.2",
    "@nuxt/core": "^2.12.2",
    "@nuxt/typescript-runtime": "0.4.6",
    "@nuxt/webpack": "^2.12.2",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/module-test-utils": "^1.6.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.3.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "bcryptjs": "^2.4.3",
    "buefy": "^0.8.18",
    "bulma": "^0.8.2",
    "bulmabuildingblocks": "^0.5.9",
    "convict": "^6.0.0",
    "convict-format-with-validator": "^6.0.0",
    "cookie-universal": "^2.1.4",
    "dotenv": "^8.2.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "nuxt": "^2.12.2",
    "nuxt-property-decorator": "^2.7.2",
    "pg": "8.2.1",
    "redirect-ssl": "^1.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.25",
    "vue": "^2.6.11",
    "vue-quill-editor": "^3.0.6",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nuxt/typescript-build": "^0.6.6",
    "@percy/cypress": "^2.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.3",
    "@types/file-saver": "^2.0.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.14",
    "@types/jquery": "^3.3.38",
    "@types/lodash": "^4.14.151",
    "@types/node": "14.0.1",
    "@types/prettier": "^2.0.0",
    "@types/quill": "^2.0.4",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/server-test-utils": "^1.0.2",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.0.1",
    "clean-css-cli": "^4.3.0",
    "codecov": "^3.6.5",
    "concurrently": "5.2.0",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "2.6.11",
    "cross-env": "^7.0.2",
    "cypress": "^4.6.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.1.0",
